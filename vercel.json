{
  "version": 2,
  "builds": [
      {
          "src": "src/index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/public/$1"
      },
      {
          "src": "/(.*)",
          "dest": "/src/index.js",
          "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
  ]
}
